diff -Nur orig/arch/powerpc/boot/dts/p2020wlan.dts dest/arch/powerpc/boot/dts/p2020wlan.dts
--- orig/arch/powerpc/boot/dts/p2020wlan.dts	1969-12-31 16:00:00.000000000 -0800
+++ dest/arch/powerpc/boot/dts/p2020wlan.dts	2012-07-13 04:24:32.949569605 -0700
@@ -0,0 +1,502 @@
+/*
+ * P2020 RDB Device Tree Source
